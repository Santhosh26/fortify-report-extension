{
  "name": "fortify-report-extension",
  "version": "9.0.0",
  "description": "Fortify SSC Report for Azure DevOps",
  "private": true,
  "author": "Santhosh Kumar",
  "license": "MIT",
  "dependencies": {
    "azure-devops-extension-api": "1.157.0",
    "azure-devops-extension-sdk": "2.0.11",
    "azure-devops-ui": "2.165.1",
    "azure-pipelines-task-lib": "4.13.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/node": "13.13.30",
    "@types/react": "16.9.25",
    "@types/react-dom": "16.9.5",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "file-loader": "6.0.0",
    "rimraf": "3.0.2",
    "sass": "^1.89.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "tfx-cli": "0.7.11",
    "ts-loader": "6.2.1",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build-task && npm run build-tab",
    "build-task": "webpack --config webpack.task.config.js",
    "build-tab": "webpack --config webpack.tab.config.js",
    "postbuild": "npm run package-extension",
    "package-extension": "tfx extension create --manifest-globs vss-extension.json",
    "dev": "webpack --config webpack.tab.config.js --watch",
    "package": "npm run build && npm run package-extension"
  }
}
