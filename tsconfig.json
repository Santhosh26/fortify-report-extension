{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2015",
    "outDir": "dist/",
    "jsx": "react",
    "declaration": false,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2015", "dom", "dom.iterable"],
    "types": [
        "react",
        "node"
    ]
  },
  "files": [
      "src/tabContent.tsx"
  ],
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}